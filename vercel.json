{
  "version": 2,
  "builds": [
    {
      "src": "api/orders.js",
      "use": "@vercel/node",
      "config": {
        "regions": ["fra1"]
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/products.json",
      "dest": "/products.json"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/orders.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}